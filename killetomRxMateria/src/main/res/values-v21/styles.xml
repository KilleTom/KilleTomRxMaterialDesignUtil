<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="RxRaisedDropPrimaryStyle" parent="Theme.AppCompat.Light">
        <!--设置点亮的动画颜色-->
        <item name="android:colorControlHighlight">#DA6954</item>
        <!--设置正常背景颜色颜色-->
        <item name="android:colorControlNormal">@color/colorAccent</item>
        <item name="android:colorControlActivated">#DA8736</item>
        <item name="android:colorButtonNormal">@color/colorAccent</item>
    </style>

    <style name="RxRaisedDropPrimaryStyle2" parent="Theme.AppCompat.Light">
        <!--设置点亮的动画颜色-->
        <item name="android:colorControlHighlight">#DA6954</item>
        <!--设置正常背景颜色颜色-->
        <item name="android:colorControlNormal">@color/permissionBgColorGreenLight</item>
        <item name="android:colorControlActivated">#DA8736</item>
        <item name="android:colorButtonNormal">@color/permissionBgColorGreenLight</item>
    </style>

    <style name="RxRaisedDropNoRippleStyle" parent="Theme.AppCompat.Light">">
        <!--设置点亮的动画颜色为透明隐藏掉波纹动画-->
        <item name="android:colorControlHighlight">@android:color/transparent</item>
    </style>

    <style name="RxRaisedDroptranslucentStyle" parent="Theme.AppCompat.Light">
        <!--设置点亮的动画颜色-->
        <item name="android:colorControlHighlight">#DA6954</item>
        <!--设置正常背景颜色颜色-->
        <item name="android:colorControlNormal">@color/dimgray</item>

    </style>

    <style name="RxRaisedDropButtonPermissionSureStyle" parent="Theme.AppCompat.Light">
        <!--设置点亮的动画颜色-->
        <item name="android:colorControlHighlight">#66CC99</item>
        <!--设置正常背景颜色颜色-->
        <item name="android:colorControlNormal">#CCFF99</item>
        <item name="android:colorButtonNormal">#CCFF99</item>
    </style>
</resources>